/**/
#include<stdio.h>
main
{
	int dni,dia,mes,anio,edad;
	char nombre[20],apellido[20];
	printf("Ingrese Nombre: ");
	scanf ("%s",&nombre);
	printf("Ingrese apellido: ");
	scanf ("%s",&apellido);
	printf("Ingrese Fecha de nacimiento:\n");
	printf("Dia: ");
	scanf("%d",&dia);
	printf("Mes: ");
	scanf("%d",&mes);
	printf("Anio: ");
	scanf("%d",&anio);
	printf("Ingrese DNI: ");
	scanf ("%d",&dni);
	if (dia >= 30 && mes >= 9 || mes > 9)
	{
		edad = (2024-anio)-1;
	}
	else
	{
		edad = (2024-anio);
	}
	printf("tienes %d anios\n");
	if (edad >= 16)
	{
		printf("Podes Votar\n");
	}
	else
	{
		printf("No podes Votar\n");
	}
	printf("%s %s, Dni: %d\n",apellido,nombre,dni);
}
